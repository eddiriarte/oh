{
    "name": "eddiriarte/oh",
    "description": "Another simple yet neat object hydrator library for PHP.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EddIriarte\\Oh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Oh\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eduardo Iriarte-Mendez",
            "email": "eddiriarte@gmail.com"
        }
    ],
    "scripts": {
        "dry-release": [
            "standard-version --dry-run"
        ],
        "release": [
            "standard-version",
            "git push --follow-tags origin main"
        ]
    },
    "require": {
        "php": "^8.1",
        "ext-ctype": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.8",
        "symfony/var-dumper": "^6.0",
        "doctrine/collections": "^1.6",
        "illuminate/collections": "^9.7"
    }
}
